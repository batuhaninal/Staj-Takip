@model List<InternshipDocument>

@foreach (var document in Model)
{
    <div>
        <span>@document.CreatedDate</span>
        <a href="@Url.Action("ShowPdf", "InternshipDocument", new { id = document.Id })">View PDF</a>

    </div>
}
